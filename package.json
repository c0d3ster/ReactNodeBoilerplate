{
  "name": "ReactNodeBoilerplate",
  "version": "1.0.0",
  "description": "React, React Router, Express, and Webpack",
  "main": "server.js",
  "scripts": {
    "dev": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "start": "npm run build && supervisor server.js",
    "start-dev": "concurrently --kill-others \"npm run dev\" \"supervisor server.js\""
  },
  "author": "Cody Douglass",
  "license": "MIT",
  "dependencies": {
    "express": "^4.16.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "postcss-loader": "^2.1.4",
    "style-loader": "^0.21.0",
    "supervisor": "^0.12.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2"
  }
}
